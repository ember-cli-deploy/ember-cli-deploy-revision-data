{
  "name": "ember-cli-deploy-revision-data",
  "version": "2.0.0",
  "description": "An ember-cli-deploy plugin to generate data about this deploy revision including a unique revision key based on the current build",
  "keywords": [
    "ember-addon",
    "ember-cli-deploy-plugin"
  ],
  "repository": "https://github.com/ember-cli-deploy/ember-cli-deploy-revision-data",
  "license": "MIT",
  "author": "Aaron Chambers and the ember-cli-deploy team",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "test": "eslint index.js lib/**/*.js tests/**/*.js && node ./tests/runner.js"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "core-object": "^3.1.5",
    "ember-cli-deploy-plugin": "^0.2.9",
    "git-repo-info": "^2.1.1",
    "minimatch": "^3.0.4",
    "rsvp": "^4.8.5",
    "simple-git": "^3.3.0"
  },
  "devDependencies": {
    "@octokit/rest": "^18.5.3",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "ember-cli": "^3.26.1",
    "eslint": "^7.25.0",
    "glob": "^7.1.6",
    "mocha": "^8.3.2",
    "release-it": "^14.6.1",
    "release-it-lerna-changelog": "^3.1.0"
  },
  "engines": {
    "node": ">= 12"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "volta": {
    "node": "14.16.1",
    "yarn": "1.22.18"
  }
}
